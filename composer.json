{
    "name": "samyan/omnipay-ecopayz",
    "type": "library",
    "description": "Ecopayz driver for the Omnipay payment processing library",
    "keywords": [
        "ecopayz",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/samyan/omnipay-ecopayz",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\Ecopayz\\": "src/"
        }
    },
    "require": {
        "omnipay/common": "^3.0"
    },
    "require-dev": {
        "omnipay/tests": "^3.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpro/grumphp": "^0.14.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "prefer-stable": true
}
